<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<ScrollPane prefHeight="695.0" prefWidth="1306.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adminController.productDeliveryController">
    <content>
        <AnchorPane prefHeight="689.0" prefWidth="1304.0">
            <children>
                <AnchorPane fx:id="acDashBord" layoutX="14.0" prefHeight="505.0" prefWidth="128.0">
                    <children>
                        <Button fx:id="btnMoreProductName" contentDisplay="TOP" mnemonicParsing="false" onAction="#getFromfromMoreProductName" prefHeight="63.0" prefWidth="127.0" text="Add Product Name" />
                        <Button fx:id="btnCategory" contentDisplay="TOP" layoutX="1.0" layoutY="74.0" mnemonicParsing="false" onAction="#getFromAddcategory" prefHeight="63.0" prefWidth="127.0" text="Add Category" />
                        <Button fx:id="btnOrders" contentDisplay="TOP" layoutY="152.0" mnemonicParsing="false" onAction="#getFromfromAddSupplier" prefHeight="63.0" prefWidth="127.0" text="Add Supplier" />
                        <Button fx:id="btnProduct" contentDisplay="TOP" layoutY="224.0" mnemonicParsing="false" onAction="#getFromAddProduct" prefHeight="63.0" prefWidth="127.0" text="Products" />
                        <Button fx:id="btnImportGoods" contentDisplay="TOP" layoutX="1.0" layoutY="297.0" mnemonicParsing="false" onAction="#getFromImportGoods" prefHeight="63.0" prefWidth="127.0" text="Import Goods" />
                        <Button fx:id="btnImportGoods1" contentDisplay="TOP" layoutX="1.0" layoutY="369.0" mnemonicParsing="false" onAction="#getFromImportGoods" prefHeight="63.0" prefWidth="127.0" text="Product Delivery" />
                        <Button fx:id="btnImportGoods11" contentDisplay="TOP" layoutY="440.0" mnemonicParsing="false" onAction="#getFromInventory" prefHeight="63.0" prefWidth="127.0" text="Inventory" />
                    </children>
                </AnchorPane>
                <Button fx:id="lblLogOut" contentDisplay="TOP" layoutX="1213.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleLogout" text="Log Out" />
                <TableView fx:id="importTable" layoutX="472.0" layoutY="314.0" prefHeight="200.0" prefWidth="399.0">
                    <columns>
                        <TableColumn fx:id="productNameColumn" prefWidth="93.0" text="Product" />
                        <TableColumn fx:id="supplierNameColumn" prefWidth="94.0" text="Supplier" />
                        <TableColumn fx:id="importDateColumn" prefWidth="105.0" text="Import Date" />
                        <TableColumn fx:id="quantityColumn" prefWidth="105.0" text="Quantity" />
                    </columns>
                </TableView>
                <Button layoutX="482.0" layoutY="539.0" mnemonicParsing="false" text="Sửa" />
                <Label layoutX="441.0" layoutY="127.0" text="Import quantity">
                    <font>
                        <Font name="Arial" size="12.0" />
                    </font>
                </Label>
                <TextField fx:id="exportQuantity" layoutX="500.0" layoutY="153.0" minWidth="50.0" prefHeight="28.0" prefWidth="217.0" />
               
                <TableView fx:id="exportTable" layoutX="256.0" layoutY="127.0" prefHeight="200.0" prefWidth="174.0">
                    <columns>
                        <TableColumn fx:id="supplierNameColumn" prefWidth="75.0" text="Nhà cung cấp" />
                        <TableColumn fx:id="productNameColumn" prefWidth="100.0" text="Tên sản phẩm" />
                        
                    </columns>
                </TableView>


                <Button layoutX="307.0" layoutY="566.0" mnemonicParsing="false" text="Add Products" />
                <Label layoutX="254.0" layoutY="60.0" prefHeight="45.0" prefWidth="178.0" text="Product Delivery">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Label>

            </children>
        </AnchorPane>
    </content>
</ScrollPane>
